# RTC
genfscon sysfs      /devices/soc/800f000.qcom,spmi/spmi-0/spmi0-00/800f000.qcom,spmi:qcom,pm660@0:qcom,pm660_rtc/rtc        u:object_r:sysfs_rtc:s0

# Touchpanel gesture nodes
genfscon proc       /touchpanel                                                                                             u:object_r:proc_touchpanel:s0

# WLAN
genfscon debugfs    /wlan0/power_stats                                                                                      u:object_r:debugfs_power_stats:s0
genfscon sysfs      /devices/soc/18800000.qcom,icnss/net/wlan0                                                              u:object_r:sysfs_wlan:s0

# P2P
genfscon sysfs      /devices/soc/18800000.qcom,icnss/net/p2p0                                                               u:object_r:sysfs_p2p0:s0
